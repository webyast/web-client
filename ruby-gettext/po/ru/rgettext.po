msgid "no input files"
msgstr "не заданы входные файлы"

#: lib/gettext/rmsgfmt.rb:47
msgid "Usage: %s input.po [-o output.mo]"
msgstr "Использование: %s input.po [-o output.mo]"

#: lib/gettext/rmsgfmt.rb:49
