<graphs type="array">
  <graph>
    <id>Network</id>
    <y_scale>1</y_scale>
    <y_label>MByte</y_label>
    <single_graphs type="array">
      <single_graph>
        <cummulated>false</cummulated>
        <linegraph>true</linegraph>
        <headline>eth0</headline>
        <lines type="array">
          <line>
            <metric_id>interface+if_packets-eth0</metric_id>
            <metric_column>rx</metric_column>
            <label>received</label>
            <limits>
              <max>0</max>
              <min>0</min>
              <reached>false</reached>
            </limits>
          </line>
          <line>
            <metric_id>interface+if_packets-eth0</metric_id>
            <metric_column>tx</metric_column>
            <label>sent</label>
            <limits>
              <max>0</max>
              <min>0</min>
              <reached>false</reached>
            </limits>
          </line>
        </lines>
      </single_graph>
    </single_graphs>
  </graph>
