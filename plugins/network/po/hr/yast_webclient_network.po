#: app/controllers/network_controller.rb:43
msgid "No permissions for network module"
msgstr "Nema dozvola za mrežni modul"

#: app/controllers/network_controller.rb:66
#| msgid "not confined\n"
msgid "Not configured"
msgstr "Nije podešeno"

#: app/controllers/network_controller.rb:66
#| msgid "Manual Mode"
msgid "Manual"
msgstr "Ručno"

#: app/controllers/network_controller.rb:66
#| msgid "Automatic Mode"
msgid "Automatic"
msgstr "Automatski"

#: app/controllers/network_controller.rb:105
#| msgid "Settings have been written."
msgid "Network settings have been written."
msgstr "Mrežne postavke su zapisane."

#: app/views/network/index.html.erb:88
msgid "Invalid IP address format. The correct one is nnn.nnn.nnn.nnn for nnn between 0 and 255."
msgstr "Neispravan format IP adrese. Ispravan je nnn.nnn.nnn.nnn za nnn između 0 i 255."

#: app/views/network/index.html.erb:97
#| msgid "Invalid Prefix. The correct one /nn where nn is between 0-32 (CIDR format)."
msgid "Invalid Prefix format. The correct one /nn where nn is between 0-32 (CIDR format)."
msgstr "Neispravan format prefiksa. Ispravan je /nn gdje je nn između 0-32 (CIDR format)."

#: app/views/network/index.html.erb:103
#| msgid "Invalid default route. The correct one is nnn.nnn.nnn.nnn for nnn between 0 and 255."
msgid "Invalid default route format. The correct one is nnn.nnn.nnn.nnn for nnn between 0 and 255."
msgstr "Neispravan format pretpostavljene rute. Ispravan je nnn.nnn.nnn.nnn gdje je nnn između 0 i 255."

#: app/views/network/index.html.erb:109
#| msgid "Name servers is not valid. There must be at least one specified."
msgid "Name servers are required. There must be at least one specified."
msgstr "Poslužitelji imena su obavezni. Mora biti naveden barem jedan."

#: app/views/network/index.html.erb:115
#| msgid "Hostname is not valid. It must be specified."
msgid "Hostname is required. It must be specified."
msgstr "Naziv računala je obavezan. Mora biti naveden."

#: app/views/network/index.html.erb:121
#| msgid "Domain is not valid. It is not supported empty domain."
msgid "Domain is required. It is not supported empty domain."
msgstr "Domena je obavezna. Prrazna domena nije podržana."

