#: app/views/network/index.html.erb:87
msgid "Invalid IP address format. The correct one is nnn.nnn.nnn.nnn for nnn between 0 and 255."
msgstr "Neispravan format IP adrese. Ispravan je nnn.nnn.nnn.nnn za nnn između 0 i 255."

#: app/views/network/index.html.erb:96
#| msgid "Invalid IP address format. The correct one is nnn.nnn.nnn.nnn for nnn between 0 and 255."
msgid "Invalid Prefix. The correct one /nn where nn is between 0-32 (CIDR format)."
msgstr "Neispravan prefix. Ispravan je /nn gdje je nn između 0-32 (CIDR format)."

#: app/views/network/index.html.erb:102
#| msgid "Invalid IP address format. The correct one is nnn.nnn.nnn.nnn for nnn between 0 and 255."
msgid "Invalid default route. The correct one is nnn.nnn.nnn.nnn for nnn between 0 and 255."
msgstr "Neispravna pretpostavljena ruta. Ispravna je nnn.nnn.nnn.nnn gdje je nnn između 0 i 255."

#: app/views/network/index.html.erb:108
msgid "Name servers is not valid. There must be at least one specified."
msgstr "Poslužitelji imena nisu ispravni. Mora biti naveden barem jedan."

#: app/views/network/index.html.erb:114
msgid "Hostname is not valid. It must be specified."
msgstr "Naziv računala nije ispravan. Mora biti naveden."

#: app/views/network/index.html.erb:120
msgid "Domain is not valid. It is not supported empty domain."
msgstr "Domena nije ispravna. Prrazna domena nije podržana."

