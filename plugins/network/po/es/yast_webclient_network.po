#: app/controllers/network_controller.rb:64
msgid "Not configured"
msgstr "Sin configurar"

#: app/controllers/network_controller.rb:64
msgid "Manual"
msgstr "Manual"

#: app/controllers/network_controller.rb:64
msgid "Automatic"
msgstr "Automático"

#: app/controllers/network_controller.rb:134
msgid "Network settings have been written."
msgstr "Los valores de red ya se han escrito."

#: app/views/network/index.html.erb:116
msgid "whitespace - invalid character."
msgstr "no es válido usar un espacio en blanco más guión (-)."

#: app/views/network/index.html.erb:118
msgid " - can not start with a digit."
msgstr " - no puede empezar por un dígito."

#: app/views/network/index.html.erb:120
msgid "periodic '-' is not allowed."
msgstr "no se permiten guiones (-) periódicos."

#: app/views/network/index.html.erb:122
msgid "can not end with (-) hyphen."
msgstr "no puede terminar en guión (-=."

#: app/views/network/index.html.erb:125
msgid "can not start with (-) hyphen."
msgstr "no puede empezar por un guión (-)."

#: app/views/network/index.html.erb:127
msgid " - is invalid character for hostname."
msgstr " el guión (-) no es un carácter válido para el nombre de host."

#: app/views/network/index.html.erb:152
msgid "Invalid IP address format. The correct one is nnn.nnn.nnn.nnn for nnn between 0 and 255."
msgstr "Formato de dirección IP no válido. El correcto es nnn.nnn.nnn.nnn, siendo nnn un número entre 0 y 255."

#: app/views/network/index.html.erb:161
msgid "Invalid Prefix format. The correct one /nn where nn is between 0-32 (CIDR format)."
msgstr "El formato del prefijo no es válido. El correcto es /nn, donde nn es un valor entre 0 y 32 (formato CIDR)."

#: app/views/network/index.html.erb:167
msgid "Invalid default route format. The correct one is nnn.nnn.nnn.nnn for nnn between 0 and 255."
msgstr "Formato de ruta por defecto IP no válido. El correcto es nnn.nnn.nnn.nnn, siendo nnn un número entre 0 y 255."

#: app/views/network/index.html.erb:173
msgid "Name servers are required. There must be at least one specified."
msgstr "Se requieren los servidores de nombres. Hay que especificar uno como mínimo."

#: app/views/network/index.html.erb:179
msgid "Hostname is required. It must be specified."
msgstr "Se requiere un nombre de host. Hay que especificarlo."

#: app/views/network/index.html.erb:185
msgid "Domain is required. An empty domain is not supported."
msgstr "Se requiere el dominio. No se admiten dominios vacíos."

