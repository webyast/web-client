msgstr "Ruční"

#: app/controllers/network_controller.rb:64
msgid "Automatic"
msgstr "Automatické"

#: app/controllers/network_controller.rb:134
msgid "Network settings have been written."
msgstr "Nastavení sítě byla zapsána."

#: app/views/network/index.html.erb:93
msgid "Invalid IP address format. The correct one is nnn.nnn.nnn.nnn for nnn between 0 and 255."
msgstr "Neplatný formát adresy IP. Správný formát je nnn.nnn.nnn.nnn, kde nnn je hodnota v rozmezí 0 až 255."

#: app/views/network/index.html.erb:102
msgid "Invalid Prefix format. The correct one /nn where nn is between 0-32 (CIDR format)."
msgstr "Neplatný formát předpony. Správný formát je /nn, kde nn je hodnota v rozmezí 0 až 32 (formát CIDR)."

#: app/views/network/index.html.erb:108
msgid "Invalid default route format. The correct one is nnn.nnn.nnn.nnn for nnn between 0 and 255."
msgstr "Neplatný formát výchozí trasy. Správný formát je nnn.nnn.nnn.nnn, kde nnn je hodnota v rozmezí 0 až 255."

#: app/views/network/index.html.erb:114
msgid "Name servers are required. There must be at least one specified."
msgstr "Jsou požadovány názvové servery. Je třeba zadat alespoň jeden."

#: app/views/network/index.html.erb:120
msgid "Hostname is required. It must be specified."
msgstr "Je požadován název hostitele. Je třeba jej zadat."

#: app/views/network/index.html.erb:126
msgid "Domain is required. An empty domain is not supported."
msgstr "Je vyžadována doména. Prázdná doména není podporována."

