#: app/controllers/network_controller.rb:64
msgid "Not configured"
msgstr "Nincs beállítva"

#: app/controllers/network_controller.rb:64
msgid "Manual"
msgstr "Kézi"

#: app/controllers/network_controller.rb:64
msgid "Automatic"
msgstr "Automatikus"

#: app/controllers/network_controller.rb:134
msgid "Network settings have been written."
msgstr "A hálózati beállítások mentése befejeződött."

#: app/views/network/index.html.erb:88
msgid ""
"Invalid IP address format. The correct one is nnn.nnn.nnn.nnn for nnn "
"between 0 and 255."
msgstr ""
"Hibás IP-cím formátum. A megfelelő formátum: nnn.nnn.nnn.nnn, ahol nnn 0 és "
"255 közé esik."

#: app/views/network/index.html.erb:97
msgid ""
"Invalid Prefix format. The correct one /nn where nn is between 0-32 (CIDR "
"format)."
msgstr ""
"Hibás prefixum. A megfelelő formátum: nn, ahol nn 0 és 32 közé esik (CIDR "
"formátum)."

#: app/views/network/index.html.erb:103
msgid ""
"Invalid default route format. The correct one is nnn.nnn.nnn.nnn for nnn "
"between 0 and 255."
msgstr ""
"Hibás alapértelmezett útválasztó. A megfelelő formátum: nnn.nnn.nnn.nnn, "
"ahol nnn 0 és 255 közé esik."

#: app/views/network/index.html.erb:109
msgid "Name servers are required. There must be at least one specified."
msgstr "A névkiszolgáló érvénytelen. Legalább egyet meg kell adnia."

#: app/views/network/index.html.erb:115
msgid "Hostname is required. It must be specified."
msgstr "Szükséges gépnevet megadni."

#: app/views/network/index.html.erb:121
msgid "Domain is required. An empty domain is not supported."
msgstr "Szükséges tartománynevet megadni. Nem támogatott az üres tartománynév."

