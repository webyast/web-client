msgstr "IPアドレス"

#: app/views/network/index.html.erb:256
msgid "Subnet mask / Prefixlen"
msgstr "サブネットマスク／プレフィクス長"

#: app/views/network/index.html.erb:260
msgid "Default route"
msgstr "デフォルトルート"

#: app/views/network/index.html.erb:269
