#: app/controllers/network_controller.rb:134
msgid "Network settings have been written."
msgstr "Setările de reţea au fost scrise."

#: app/views/network/index.html.erb:118
msgid "whitespace - invalid character."
msgstr "spaţii albe - caractere invalide."

#: app/views/network/index.html.erb:120
#| msgid "The VM's name cannot start with a digit."
msgid " - can not start with a digit."
msgstr " - nu poate începe cu o cifră."

#: app/views/network/index.html.erb:122
#| msgid "The option 'flush' is not allowed"
msgid "periodic '-' is not allowed."
msgstr "semnul '-' nu este permis."

#: app/views/network/index.html.erb:124
msgid "can not end with (-) hyphen."
msgstr "nu se poate termina cu (-) cratimă."

#: app/views/network/index.html.erb:127
msgid "can not start with (-) hyphen."
msgstr "nu poate începe cu (-) cratimă."

#: app/views/network/index.html.erb:129
msgid " - is invalid character for hostname."
msgstr " - este caracter invalid pentru numele gazdei."

#: app/views/network/index.html.erb:154
msgid ""
"Invalid IP address format. The correct one is nnn.nnn.nnn.nnn for nnn between "
"0 and 255."
msgstr ""
"Formatul adresei IP este invalid. Corect este nnn.nnn.nnn.nnn pentru nnn "
"între 0 şi 255."

#: app/views/network/index.html.erb:163
msgid ""
"Invalid Prefix format. The correct one /nn where nn is between 0-32 (CIDR "
"format)."
msgstr ""
"Formatul prefixului este invalid. Corect este /nn unde nn este cuprins între "
"0-32 (format CIDR)."

#: app/views/network/index.html.erb:169
msgid ""
"Invalid default route format. The correct one is nnn.nnn.nnn.nnn for nnn "
"between 0 and 255."
msgstr ""
"Formatul implicit al rutei este invalid. Corect este nnn.nnn.nnn.nnn pentru "
"nnn între 0 şi 255."

#: app/views/network/index.html.erb:175
msgid "Name servers are required. There must be at least one specified."
msgstr ""
"Serverele de nume sunt necesare. Trebuie să existe cel puţin unul specificat."

#: app/views/network/index.html.erb:181
msgid "Hostname is required. It must be specified."
msgstr "Numele gazdă este necesar. Acesta trebuie să fie specificat."

#: app/views/network/index.html.erb:187
#| msgid "Domain is required. It is not supported empty domain."
msgid "Domain is required. An empty domain is not supported."
msgstr "Un domeniu este necesar. Un domeniu gol nu este acceptat."

