#: app/views/eulas/show.html.erb:43
msgid "Yes, I agree to the license agreement."
msgstr "Sí, acepto el acuerdo de licencia."

#: app/views/eulas/show.html.erb:47
msgid "No, I do not agree."
msgstr "No, no lo acepto."

#: app/views/eulas/show.html.erb:58
msgid "Back"
msgstr "Atrás"

#: app/views/eulas/all_accepted.erb:2
