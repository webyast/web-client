#: app/controllers/administrator_controller.rb:32
#: app/views/administrator/index.html.erb:24
msgid "Enter a valid e-mail address."
msgstr "បញ្ចូល​អាសយដ្ឋាន​​អ៊ីមែល​ត្រឹម​ត្រូវ ។"

#: app/controllers/administrator_controller.rb:40
msgid "Passwords do not match."
msgstr "ពាក្យ​សម្ងាត់​មិន​ដូច​គ្នា​ទេ ។"

#: app/controllers/administrator_controller.rb:53
msgid "Administrator settings have been written."
msgstr "កា​រកំណត់​របស់​អ្នក​គ្រប់គ្រង​ត្រូវ​បាន​សរសេរ ។"

#: app/controllers/administrator_controller.rb:63
msgid "Error while saving administrator settings: %s"
msgstr "មាន​កំហុស ខណៈ​ពេលការ​រក្សាទុក​ការ​កំណត់​របស់​អ្នក​គ្រប់​គ្រង​ ៖ %s"

#: app/controllers/administrator_controller.rb:75
msgid "Mail alias was set but outgoing mail server is not configured (%s<i>change</i>%s)."
msgstr "ឈ្មោះ​ក្លែងក្លាយ​សំបុត្រ​ត្រូវ​បាន​កំណត់ ប៉ុន្តែ​ម៉ាស៊ីន​បម្រើ​សំបុត្រ​ចេញ​មិន​ត្រូវ​បាន​កំណត់​រចនាសម្ព័ន្ធ​ទេ (%s<i>ផ្លាស់ប្ដូរ</i>%s) ។"

#: app/views/administrator/index.html.erb:1
msgid "Administrator Settings"
msgstr "ការ​កំណត់​របស់​អ្នក​គ្រប់​គ្រង​"

#: app/views/administrator/index.html.erb:32
msgid "The passwords do not match."
msgstr "ពាក្យ​សម្ងាត់​មិន​ផ្គូផ្គង​គ្នា​​ទេ ។"

#: app/views/administrator/index.html.erb:33
msgid "A password is required."
msgstr "ត្រូវការ​ពាក្យ​សម្ងាត់ ។"

#: app/views/administrator/index.html.erb:38
msgid "Password Strength:"
msgstr "ភាព​ខ្លាំង​របស់​ពាក្យ​សម្ងាត់ ៖"

#: app/views/administrator/index.html.erb:40
msgid "Weak"
msgstr "ខ្សោយ"

#: app/views/administrator/index.html.erb:41
msgid "Normal"
msgstr "ធម្មតា"

#: app/views/administrator/index.html.erb:42
msgid "Medium"
msgstr "មធ្យម​"

#: app/views/administrator/index.html.erb:43
msgid "Strong"
msgstr "ខ្លាំង"

#: app/views/administrator/index.html.erb:44
msgid "Very Strong"
msgstr "ខ្លាំង​ណាស់"

#: app/views/administrator/index.html.erb:63
msgid "Password Setup for User 'root'"
msgstr "ការ​រៀបចំ​ពាក្យ​សម្ងាត់​សម្រាប់​អ្នក​ប្រើ 'root'"

#: app/views/administrator/index.html.erb:64
msgid "New Password"
msgstr "ពាក្យ​សម្ងាត់​ថ្មី"

#: app/views/administrator/index.html.erb:69
msgid "Confirm Password"
msgstr "បញ្ជាក់​ពាក្យ​សម្ងាត់"

#: app/views/administrator/index.html.erb:76
msgid "Mail Setup for User 'root'"
msgstr "ការ​រៀបចំ​សំបុត្រ​សម្រាប់​អ្នក​ប្រើ 'root'"

#: app/views/administrator/index.html.erb:78
msgid "Forward system mail to"
msgstr "បញ្ជូន​សំបុត្រ​ប្រព័ន្ធ​ទៅ​"

#: app/views/administrator/index.html.erb:81
msgid "Save Mail Only"
msgstr "ទុក​តែ​សំបុត្រ​ប៉ុណ្ណោះ​"
