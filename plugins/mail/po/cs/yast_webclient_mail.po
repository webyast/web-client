#: app/controllers/mail_controller.rb:28
msgid "Passwords do not match."
msgstr "Hesla se neshodují."

#: app/controllers/mail_controller.rb:35
msgid "Mail settings have been written."
msgstr "Nastavení pošty byla zapsána."

#: app/controllers/mail_controller.rb:37
msgid "Test mail was sent to %s."
msgstr "Testovací e-mail byl odeslán na adresu %s."

#: app/controllers/mail_controller.rb:49
msgid "Error while saving mail settings: %s"
msgstr "Při zápisu nastavení pošty došlo k chybě: %s"

#: app/controllers/mail_controller.rb:66
msgid "No outgoing mail server is set, but administrator has mail forwarders defined.\nChange %s<i>administrator</i>%s or %s<i>mail</i>%s configuration."
msgstr "Není nastaven server odchozí pošty, ale správce definoval servery pro předávání.\nZměňte konfiguraci %s<i>správce</i>%s nebo %s<i>pošty</i>%s."

#: app/views/mail/index.html.erb:19
msgid "Enter a valid domain name or an IP address."
msgstr "Zadejte platný název domény nebo adresu IP."

#: app/views/mail/index.html.erb:26
msgid "The passwords do not match."
msgstr "Hesla se neshodují."

#: app/views/mail/index.html.erb:27
msgid "Enter a valid e-mail address."
msgstr "Zadejte platnou e-mailovou adresu."

#: app/views/mail/index.html.erb:34
msgid "Mail Settings"
msgstr "Nastavení pošty"

#: app/views/mail/index.html.erb:44
msgid "Outgoing mail server"
msgstr "Server odchozí pošty"

#: app/views/mail/index.html.erb:47
msgid "Use secure connection (TLS)"
msgstr "Použít zabezpečené připojení (TSL)"

#: app/views/mail/index.html.erb:48
msgid "Never"
msgstr "Nikdy"

#: app/views/mail/index.html.erb:48
msgid "If available"
msgstr "Je-li k dispozici"

#: app/views/mail/index.html.erb:48
msgid "Always"
msgstr "Vždy"

#: app/views/mail/index.html.erb:50
msgid "User name"
msgstr "Uživatelské jméno"

#: app/views/mail/index.html.erb:53
msgid "Password"
msgstr "Heslo"

#: app/views/mail/index.html.erb:56
